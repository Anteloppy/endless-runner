<Window x:Class="endless_runer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:endless_runer"
        mc:Ignorable="d"
        Title="Endless runer" Height="450" Width="800">
    <Canvas Name="canvas" Focusable="True" KeyDown="canvas1_KeyDown" KeyUp="canvas1_KeyUp" Background="Aquamarine">
        
        <Rectangle Name="bg1" Height="400" Width="1000" Fill="Orange"/>
        <Rectangle Name="bg2" Height="400" Width="1000" Fill="Purple" Canvas.Left="1000"/>
        <Rectangle Name="ground" Height="32" Width="800" Canvas.Top="387" Canvas.Left="0" Fill="Black"/>
        <Rectangle Name="player" Height="99" Width="67" Fill="Bisque" Canvas.Top="150" Canvas.Left="50"/>
        <Rectangle Name="obstcle" Height="178" Width="50" Canvas.Top="320" Canvas.Left="721" Fill="Red"/>
        <Label Name="scoretext" FontSize="24" Content="Score: 0" FontFamily="comfortaa"/>
    </Canvas>
</Window>
